#header
  display flex
  justify-content space-between
  padding 6px
  background #FFF
  border-bottom 1px solid #EEE
  position absolute
  top 0
  right 0
  left 0
  height 38px
  h1
    font-size 16px
    margin 0
    line-height 20px
    margin-left 7px
    color #5A5A5A
    padding-top 3px
    font-weight normal
  .ui.icon.input > i.icon
    height 95%
